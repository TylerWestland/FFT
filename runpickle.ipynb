    "import pickle\n",
    "import numpy as np\n",
    "from IPython.display import Audio\n",
    "\n",
    "with open('FFT.pickle', 'rb') as data:\n",
    "    dataset = pickle.load(data)\n",
    "\n",
    "signal = np.fft.ifft(dataset)\n",
    "display(Audio(signal.T, rate=48000))"
   ]
  }
